FROM python:3.9.1

RUN apt-get install wget
RUN pip install pandas sqlalchemy psycopg2

WORKDIR /app
COPY ingest_data.py ingest_data.py 

ENTRYPOINT [ "python", "ingest_data.py" ]




docker pull dpage/pgadmin4
docker run -p 80:80 \
    -p 443:443 \
    -e 'PGADMIN_DEFAULT_EMAIL=farooq.ahmad.digifloat@gmail.com' \
    -e 'PGADMIN_DEFAULT_PASSWORD=root' \
    --network=pg-network \
    --name pgadmin4 \
     dpage/pgadmin4



docker run  -e POSTGRES_USER="root" -e POSTGRES_PASSWORD="root" -e POSTGRES_DB="ny_taxi" -v 'd:/no_backup/postgres_backup:/var/lib/postgresql/data' -p 5432:5432 --network=pg-network  --name pg-database postgres:13


